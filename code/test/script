OUTPUT_FORMAT("ecoff-littlemips")
ENTRY(__start)
SECTIONS
{
  .text  0 : {
     _ftext = . ;
    *(.init)
     eprol  =  .;
    *(.text*)
    *(.fini)
    . = ALIGN(128);
  }
   etext  =  .;
   _etext  =  .;
   _fdata = .;
  .data  . : {
    *(.rdata)
    *(.rodata)
    *(.sdata)
    *(.data)
    CONSTRUCTORS
    . = ALIGN(128);
  }
   edata  =  .;
   _edata  =  .;
   _fbss = .;
  .sbss  . : {
    *(.sbss)
    *(.scommon)
    . = ALIGN(128);
  }
  .bss  . : {
    *(.bss)
    *(COMMON)
    . = ALIGN(128);
  }
  .drop . : {
    *(.reginfo)
    *(.pdr)
    *(.MIPS.ab*)
    *(.comment*)
    *(.gnu.att*)
  }
   end = .;
   _end = .;
}

